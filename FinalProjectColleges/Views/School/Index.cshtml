@model IEnumerable<School>

<h2>Schools</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>SchoolName</th>
        <th>Location</th>
        <th>Enrollment</th>
        <th>Type</th>
        <th></th>
    </tr>

    @foreach (var School in Model)
    {
<tr>

    <td><a href=/School/ViewSchool/@School.ID>@School.ID</a></td>
    <td>@School.SchoolName</td>
    <td>@School.Location</td>
    <td>@School.StudentCount</td>
    <td>@School.Type</td>
    <td>@School.Pic  <img src="Images/College .jpeg" style="width:100%"></td>

</tr>

    }
</table>
